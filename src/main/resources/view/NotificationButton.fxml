<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="buttonPane" maxHeight="19.0" prefHeight="19.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="buttonIcon" fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
         <image>
            <Image url="@../images/bell_icon.png" />
         </image>
      </ImageView>
      <Button fx:id="notifButton" maxHeight="19.0" mnemonicParsing="false" prefHeight="19.0" />
   </children>
</StackPane>
