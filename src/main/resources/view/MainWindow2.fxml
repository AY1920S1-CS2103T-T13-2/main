<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<fx:root height="650.0" minHeight="650.0" minWidth="1250.0" onCloseRequest="#handleExit" title="Address App" type="javafx.stage.Stage" width="1250.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/address_book_32.png" />
  </icons>
  <scene>
    <Scene fill="#161d33">
      <stylesheets>
        <URL value="@DarkTheme.css" />
        <URL value="@Extensions.css" />
      </stylesheets>

      <VBox maxWidth="1250.0" prefHeight="650.0" prefWidth="1250.0">
            <AnchorPane prefWidth="1250.0" VBox.vgrow="NEVER">
               <children>
              <MenuBar fx:id="menuBar" maxHeight="26.0" prefHeight="26.0" prefWidth="1250.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <Menu mnemonicParsing="false" text="File">
                  <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
                </Menu>
                  <Menu mnemonicParsing="false" text="Help">
                      <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
                  </Menu>
              </MenuBar>
                  <StackPane fx:id="notificationButtonPlaceholder" alignment="TOP_CENTER" AnchorPane.rightAnchor="275.0" />
               </children>
            </AnchorPane>
            <HBox maxHeight="450.0" prefHeight="450.0" prefWidth="1250.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox minWidth="1000.0" prefHeight="450.0" prefWidth="1000.0" HBox.hgrow="ALWAYS">
                     <children>
                        <StackPane fx:id="bodyMasterListPlaceholder" maxHeight="350.0" prefHeight="350.0" styleClass="stack-pane">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <effect>
                               <DropShadow color="#0000001a" height="25.0" radius="12.0" spread="0.68" width="25.0" />
                           </effect>
                        </StackPane>
                      <StackPane fx:id="lineChartPanelPlaceholder" maxHeight="100.0" prefHeight="100.0" styleClass="stack-pane">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                          <effect>
                              <DropShadow color="#0000001a" height="25.0" radius="12.0" spread="0.68" width="25.0" />
                          </effect>
                      </StackPane>
                     </children>
                  </VBox>
              <VBox fx:id="personList" maxWidth="250.0" prefHeight="450.0" prefWidth="250.0">
                <StackPane fx:id="personListPanelPlaceholder" prefHeight="450.0" styleClass="stack-pane">
                        <VBox.margin>
                           <Insets />
                        </VBox.margin>
                        <effect>
                            <DropShadow color="#0000001a" height="25.0" radius="12.0" spread="0.68" width="25.0" />
                        </effect>
                     </StackPane>
              </VBox>
               </children>
            </HBox>

              <StackPane fx:id="resultDisplayPlaceholder" maxHeight="120.0" prefHeight="100.0" styleClass="stack-pane" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
              <effect>
                  <DropShadow color="#0000001a" height="25.0" radius="12.0" spread="0.68" width="25.0" />
              </effect>
        </StackPane>

                 <StackPane fx:id="commandBoxPlaceholder" styleClass="stack-pane" VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
                 <effect>
                     <DropShadow color="#0000001a" height="25.0" radius="12.0" spread="0.68" width="25.0" />
                 </effect>
        </StackPane>
        <StackPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
      </VBox>
    </Scene>
  </scene>
</fx:root>
